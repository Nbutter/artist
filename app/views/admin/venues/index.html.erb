<p id="notice"><%= notice %></p>

<h1>Listing Venues</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>City</th>
      <th>Country</th>
      <th>Website url</th>
      <th>Image url</th>
      <th>Image url2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venues.each do |venue| %>
      <tr>
        <td><%= venue.name %></td>
        <td><%= venue.city %></td>
        <td><%= venue.country %></td>
        <td><%= link_to venue.website_url, venue.website_url if venue.website_url  %></td>
        <td><%= link_to venue.image_url, venue.image_url if venue.image_url %></td>
        <td><%= link_to venue.image_url2, venue.image_url2 if venue.image_url2 %></td>
        <td><%= link_to 'Show', venue %></td>
        <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
        <td><%= link_to 'Destroy', venue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venue', new_admin_venue_path %>
